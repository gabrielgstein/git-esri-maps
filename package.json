{
  "name": "esri-maps",
  "version": "0.7.0",
  "description": "bf-esri-map React component",
  "main": "./index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm-run-all build-css build-nwb",
    "build-nwb": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "npm-run-all -p watch-css start-nwb",
    "start-nwb": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "build-css": "node-sass-chokidar src/ -o src/ && node-less-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "antd": "3.7.3",
    "axios": "^0.16.2",
    "chart.js": "^2.7.0",
    "chroma-js": "^1.3.4",
    "esri-loader": "^1.0.0",
    "esri-loader-react": "^1.0.1",
    "geostats": "^1.5.0",
    "immutability-helper": "^2.3.0",
    "material-ui": "=1.0.0-beta.8",
    "material-ui-icons": "=1.0.0-beta.5",
    "object-hash": "^1.2.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-slider": "^8.3.1",
    "react": "16.3.2",
    "react-addons-update": "^15.4.2",
    "react-color": "^2.14.1",
    "react-dom": "16.3.2",
    "react-redux": "^5.0.5",
    "react-sortable-hoc": "^0.6.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "string-mask": "^0.3.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "less": "^3.0.1",
    "node-less": "^1.0.0",
    "node-less-chokidar": "^0.1.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "nwb": "0.18.x",
    "nwb-sass": "^0.8.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-scripts": "1.0.10",
    "style-loader": "^0.20.3"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ]
}
